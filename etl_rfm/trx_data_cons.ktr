    <connection>jump server</connection>
    <sql>select  
acc_identification,ISSUER,
TRX_DATE, 
BUSINESS_DATE,
STATUS,
RESP_CODE,
MCC,
TRANSACTION_TYPE,
SWITCH_ISS,
SWITCH_ACQ,
IS_ONNET,
AMOUNT,
MERCHANT_CRITERIA,
(SELECT MAINTAB.amount*(MDR/100)*ISSUER_FEE FROM qrpayment.QR_MDR QM WHERE QM.EFFECTIVE_DATE &lt;= MAINTAB.BUSINESS_DATE AND QM.FEATURES_ID=MAINTAB.PROC_CODE AND QM.MERCHANT_CODE=MAINTAB.MERCHANT_CODE AND QM.MERCHANT_CRITERIA =MAINTAB.MERCHANT_CRITERIA ORDER BY QM.EFFECTIVE_DATE DESC LIMIT 1) ISSUER_FEE,
(SELECT MAINTAB.amount*(MDR/100)*ACQUIRER_FEE FROM qrpayment.QR_MDR QM WHERE QM.EFFECTIVE_DATE &lt;= MAINTAB.BUSINESS_DATE AND QM.FEATURES_ID=MAINTAB.PROC_CODE AND QM.MERCHANT_CODE=MAINTAB.MERCHANT_CODE AND QM.MERCHANT_CRITERIA =MAINTAB.MERCHANT_CRITERIA ORDER BY QM.EFFECTIVE_DATE DESC LIMIT 1) ACQUIRER_FEE, 
(SELECT MAINTAB.amount*(MDR/100)*SERVICES_FEE FROM qrpayment.QR_MDR QM WHERE QM.EFFECTIVE_DATE &lt;= MAINTAB.BUSINESS_DATE AND QM.FEATURES_ID=MAINTAB.PROC_CODE AND QM.MERCHANT_CODE=MAINTAB.MERCHANT_CODE AND QM.MERCHANT_CRITERIA =MAINTAB.MERCHANT_CRITERIA ORDER BY QM.EFFECTIVE_DATE DESC LIMIT 1) PTEN_FEE, 
(SELECT MAINTAB.amount*(MDR/100)*STANDARDS_FEE FROM qrpayment.QR_MDR QM WHERE QM.EFFECTIVE_DATE &lt;= MAINTAB.BUSINESS_DATE AND QM.FEATURES_ID=MAINTAB.PROC_CODE AND QM.MERCHANT_CODE=MAINTAB.MERCHANT_CODE AND QM.MERCHANT_CRITERIA =MAINTAB.MERCHANT_CRITERIA ORDER BY QM.EFFECTIVE_DATE DESC LIMIT 1) ASPI_FEE, 
case when IS_ONNET = '1' then (SELECT MAINTAB.amount*(MDR/100) * SWITCHING_FEE FROM qrpayment.QR_MDR QM WHERE QM.EFFECTIVE_DATE &lt;= MAINTAB.BUSINESS_DATE AND QM.FEATURES_ID=MAINTAB.PROC_CODE AND QM.MERCHANT_CODE=MAINTAB.MERCHANT_CODE AND QM.MERCHANT_CRITERIA =MAINTAB.MERCHANT_CRITERIA ORDER BY QM.EFFECTIVE_DATE DESC LIMIT 1) else (SELECT MAINTAB.amount*(MDR/100) *SWITCHING_FEE*0.5 FROM qrpayment.QR_MDR QM WHERE QM.EFFECTIVE_DATE &lt;= MAINTAB.BUSINESS_DATE AND QM.FEATURES_ID=MAINTAB.PROC_CODE AND QM.MERCHANT_CODE=MAINTAB.MERCHANT_CODE AND QM.MERCHANT_CRITERIA =MAINTAB.MERCHANT_CRITERIA ORDER BY QM.EFFECTIVE_DATE DESC LIMIT 1) end JALIN_FEE, 
case when IS_ONNET = '1' then 0 else (SELECT MAINTAB.amount*(MDR/100) * SWITCHING_FEE*0.5 FROM qrpayment.QR_MDR QM WHERE QM.EFFECTIVE_DATE &lt;= MAINTAB.BUSINESS_DATE AND QM.FEATURES_ID=MAINTAB.PROC_CODE AND QM.MERCHANT_CODE=MAINTAB.MERCHANT_CODE AND QM.MERCHANT_CRITERIA =MAINTAB.MERCHANT_CRITERIA ORDER BY QM.EFFECTIVE_DATE DESC LIMIT 1) end OTHERS_SWITCH_FEE
from (
SELECT  
acc_identification,
TRX_DATE, 
BUSINESS_DATE,
CASE 
WHEN ISSUER IN('SAT','RTS','ALT') THEN (select MEMBER_ALIAS from qrpayment.PRINCIPAL_MEMBER where MEMBER_CODE = BIT32)
ELSE ISSUER END 
ISSUER, 
TRX_STS STATUS,
RESP_CODE,
CHANNEL_TYPE_ID CHANNEL,
POS_MCC MCC,
case
when (SELECT mm.CATEGORY_CODE FROM qrpayment.MCC_MAP mm WHERE tdc.POS_MCC=mm.MCC) is null then '1'
else (SELECT mm.CATEGORY_CODE FROM qrpayment.MCC_MAP mm WHERE tdc.POS_MCC=mm.MCC) end
MERCHANT_CODE,
PROC_CODE,
(select TRX_ALIAS FROM qrpayment.TRX_MAP tm WHERE SUBSTRING(tdc.PROC_CODE,1,2)=tm.PROC_CODE) TRANSACTION_TYPE,
SWITCH_ISS_CODE SWITCH_ISS,
SWITCH_ACQ_CODE SWITCH_ACQ,
CASE 
when SWITCH_ISS_CODE = SWITCH_ACQ_CODE then '1'
when SWITCH_ISS_CODE &lt;&gt; SWITCH_ACQ_CODE then '0'
else null
end IS_ONNET,
AMOUNT+0 AMOUNT,
MERCHANT_CRITERIA
FROM qrpayment.TRX_DATA_CONS tdc)MAINTAB
Where BUSINESS_DATE BETWEEN ${DATE_VAL1} AND ${DATE_VAL2};</sql>
